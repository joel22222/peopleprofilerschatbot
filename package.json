    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "elevenlabs-node": "^1.2.0",
    "express": "^4.18.2",
    "next": "^14.0.0",
    "openai": "^4.26.0",
    "say": "^0.16.0"
